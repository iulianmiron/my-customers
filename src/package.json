    "@uirouter/angularjs": "^1.0.6",
    "angular": "^1.6.6",
    "angular-animate": "^1.6.6",
    "angular-aria": "^1.6.6",
    "angular-hotkeys": "^1.7.0",
    "angular-material": "^1.1.5",
    "angular-messages": "^1.6.6",
    "angular-moment": "^1.0.1",
    "angular-sanitize": "^1.6.6",
    "angular-spinners": "^3.1.2",
